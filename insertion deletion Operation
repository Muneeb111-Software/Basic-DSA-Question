#include<iostream>
#include<bits/stdc++.h>
#include<list>
using namespace std;
int main(){
	list<int> demolist;
	demolist.push_back(10);
	demolist.push_front(1);
	demolist.push_back(20);
	demolist.push_back(25);
	demolist.push_back(30);
	cout<<"List is : ";
	for(auto itr =demolist.begin();itr != demolist.end();
	itr++)
	cout<< *itr<<" ";
	demolist.pop_back();
	cout<<"\n List after removing an element from end: ";
	for(auto itr =demolist.begin();itr!=demolist.end();itr++)
	cout<<*itr<<" ";
	return 0;
}
